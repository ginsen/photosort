{
    "name": "ginsen/photosort",
    "description": "A small app to rename images files by created date",
    "homepage": "https://github.com/ginsen/photosort",
    "keywords": ["console", "filesystem", "php"],
    "license": "MIT",
    "authors": [
        {
            "name": "J. Ginés Hernández G.",
            "email": "jgines@gmail.com",
            "homepage": "https://about.me/jgines",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.0"
    },
    "autoload": {
        "psr-4": { "PhotoSort\\": "src/PhotoSort/" }
    },
    "support": {
        "issues": "https://github.com/ginsen/photosort/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "vendor"
    },
    "bin": [
        "bin/photosort.phar",
        "photosort"
    ]
}
