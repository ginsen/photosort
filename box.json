{
  "directories": ["src/"],
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        ".gitignore",
        ".md",
        "phpunit",
        "Tester",
        "Tests",
        "tests",
        "yaml"
      ],
      "in": "vendor"
    }
  ],
  "compactors": [
    "Herrera\\Box\\Compactor\\Php",
    "Herrera\\Box\\Compactor\\Json"
  ],
  "compression": "GZ",
  "git-version": "package_version",
  "main": "photosort",
  "output": "bin/photosort.phar",
  "stub": true,
  "chmod": "0755"
}